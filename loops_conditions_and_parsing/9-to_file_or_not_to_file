#!/usr/bin/env bash
#COmment
FILE=/home/vagrant/shell/loops_conditions_and_parsing/school

if test -f "$FILE";
then
echo "school file exists"
echo "school is a regular file"
   else
        echo "school file does not exists"
   fi
    if test -s "$FILE";
   then
        echo "school file is empty"
   else
        echo "school file is not empty"
   fi
if test -f "$FILE";
then
	echo "school is a regular file"
fi
